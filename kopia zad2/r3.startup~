ifconfig eth0 192.168.192.22 netmask 255.255.255.252 broadcast 192.168.192.23 up
ifconfig eth1 192.168.192.29 netmask 255.255.255.252 broadcast 192.168.192.30 up

route add -net 192.168.192.0 netmask 255.255.255.252 gw 192.168.192.21 dev eth0
route add -net 192.168.192.12 netmask 255.255.255.252 gw 192.168.192.21 dev eth0
route add -net 192.168.192.24 netmask 255.255.255.252 gw 192.168.192.30 dev eth1
route add -net 192.168.194.0 netmask 255.255.255.0 gw 192.168.192.30 dev eth1
